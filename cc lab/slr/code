+;
for(j=0;j<t;j++){
if(stack[k] == ter[j]) {
row = j;
}
}
   top++;
stack[top] = table[col][row][0];
printf("%c\n",table[col][row][0]);
}
else{
printf("\nError\nThe string not accepted");
break;
}
printf("\n");
for(j=0;j<=top;j++) {
printf("%c",stack[j]);
}
printf("\t \t");
for(j=1;j<strlen(ip);j++) {
printf("%c",ip[j]);
}
printf("\t\t");
}
